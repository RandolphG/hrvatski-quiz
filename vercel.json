{ "version": 2,
  "builds": [
    {
      "src": "./server/index.js",
      "use": "@now/node",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "./server/index.js"
    }
  ]
}